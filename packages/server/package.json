    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@ai-chatbox/mcp-core": "workspace:*",
    "@ai-chatbox/shared": "workspace:*",
    "@hono/node-server": "^1.13.7",
    "dotenv": "^17.2.3",
    "hono": "^4.6.14",
    "openai": "^4.77.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
